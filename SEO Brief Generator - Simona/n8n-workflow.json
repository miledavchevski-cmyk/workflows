      ],
      "id": "56f0bacd-dfaf-4734-a907-a0c7232febee",
      "name": "Prepare AI Prompt",
      "disabled": true
    },
    {
      "parameters": {
        "jsCode": "// Get the current input from Gemini\nconst promptResponse = $input.first().json;\n\n// Extract the text from Gemini's response\n// Correct path: content.parts[0].text\nlet contentBrief = '';\ncontentBrief = promptResponse.output;\n\n// Find the keyword from previous steps\nlet keyword = $(\"Enter Keyword\").first().json.keyword\n\n\n\n// Format the output\nreturn [{\n  json: {\n    keyword: keyword,\n    contentBrief: contentBrief,\n    analyzedAt: new Date().toISOString(),\n    briefLength: contentBrief.length,\n    summary: `Content brief for \"${keyword}\" is ready! Check the contentBrief field for detailed recommendations.`\n  }\n}];"
      },
