    {
      "parameters": {
        "jsCode": "// Get the search results from previous node\nconst searchResults = $input.first().json;\n\n// Extract only the organic results\nconst organicResults = searchResults.organic || [];\n\n// Take only the top 5 results (to save time)\nconst topResults = organicResults.slice(0, 5);\n\n// Create output with just the URLs\nconst urls = topResults.map((result, index) => ({\n  json: {\n    url: result.link,\n    title: result.title,\n    position: index + 1\n  }\n}));\n\nreturn urls;"
      },
